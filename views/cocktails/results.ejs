<%-include('../partials/header.ejs', {user}) %>

<%-include('../partials/form.ejs') %>

<% results.forEach(result =>{%>
   <h2><%= result.name %></h2>
   <form method="POST" action="/cocktails/favorites">
           <button class="btn btn-primary" type="submit">Favorite🍸</button>
    </form>
<ul>
   <%result.ingredients.forEach(ingredient=>{ %>
<li >
   <%=ingredient%>
</li>

   <%})%>
</ul>
   <p> Instructions: <%=result.instructions%></p>

    
<%})%>


<%- include('../partials/footer.ejs') %>
